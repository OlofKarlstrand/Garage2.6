@model IEnumerable<Garage2._6.Models.ParkedVehicle>

@{
    ViewBag.Title = "Overview";
}

<h2>Overview</h2>

<p>
    @Html.ActionLink("Park a Vehicle", "Park")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegNr)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>

   

        <th>
            @Html.DisplayNameFor(model => model.CheckIn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegNr)
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>

       
        <td>
            @Html.DisplayFor(modelItem => item.CheckIn)
        </td>
        <td>
     
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Check Out", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
