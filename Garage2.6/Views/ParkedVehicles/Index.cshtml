@model IEnumerable<Garage2._6.Models.ParkedVehicle>


<h2>Full Index</h2>

<p>
    @Html.ActionLink("Create New", "Park")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegNr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Color)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberofEngines)
        </th>
        <th>@Html.DisplayNameFor(model => model.DropType)</th>

        <th>@Html.DisplayNameFor(model => model.Type)</th>
        <th>
            @Html.DisplayNameFor(model => model.CheckIn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegNr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Color)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberofEngines)
        </td>
        <td>@Html.DisplayFor(modelItem => item.DropType)</td>
        <td>@Html.DisplayFor(modelItem=> item.Type)</td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckIn)
        </td>
      
    </tr>
}

</table>
